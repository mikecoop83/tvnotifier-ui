		"@skeletonlabs/skeleton": "^2.11.0",
		"@types/lodash": "^4.14.191",
		"base64url": "^3.0.1",
		"googleapis": "^111.0.0",
		"jsonwebtoken": "^9.0.0",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"phosphor-svelte": "^1.2.1",
		"pnpm": "^10.24.0"
